---
import TitlePage from "@/components/common/TitlePage.astro";
import Layout from "@/layouts/Layout.astro";
import { CheckoutForm } from "@/components/recaptchaV3/ejemplosDeUsos/sections/EcommerceCheckoutExample";
import { useTranslations } from "@/i18n/utils";

const t = useTranslations("en");
---

<Layout
  titleKey="page.usage-examples.title"
  descriptionKey="seo.description"
  keywords={t("seo.keywords")}
  image="/og-image.jpg"
  publishDate="2025-01-01"
  modifiedDate="2025-01-01"
  type="website"
  twitterHandle="@AndresH11"
>
  <main class="py-14 px-6 md:px-12 space-y-11">
    <TitlePage title={t("examples.ecommerce-checkout")} />
    <CheckoutForm client:visible />
  </main>
</Layout>

